coroutine.wrap(SKDTGMS_fake_script)()
local function RCWIDT_fake_script() -- Window.DragWindowScript 
	local script = Instance.new('LocalScript', Window)

	local UserInputService = game:GetService("UserInputService")
	local gui = script.Parent
	local dragButton = gui:WaitForChild("DragWindow") -- Assuming there's a button named DragWindow
	
	local dragging = false
	local dragStart
	local startPos
	
	local function update(input)
	    local delta = input.Position - dragStart
	    gui.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
	end
	
	dragButton.InputBegan:Connect(function(input)
	    if input.UserInputType == Enum.UserInputType.MouseButton1 then
	        dragging = true
	        dragStart = input.Position
	        startPos = gui.Position
	
	        input.Changed:Connect(function()
	            if input.UserInputState == Enum.UserInputState.End then
	                dragging = false
	            end
	        end)
	    end
	end)
	
	UserInputService.InputChanged:Connect(function(input)
	    if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
	        update(input)
	    end
	end)
	
	
end
coroutine.wrap(RCWIDT_fake_script)()
local function BYMQJ_fake_script() -- DropdownButton.LocalScript 
	local script = Instance.new('LocalScript', DropdownButton)

	-- Importowanie usług
	local TweenService = game:GetService("TweenService")
	local Players = game:GetService("Players")
	local VirtualInputManager = game:GetService("VirtualInputManager")
	local GuiService = game:GetService("GuiService")
	
	-- Referencje
	local button = script.Parent -- TextButton
	local frame = script.Parent.Parent.Parent.DropdownFrame1 -- Poprawne odniesienie do ramki tła
	local clickButton = frame.ClickButon
	-- Zmienna przechowująca stan toggle
	local isActive = false
	
	-- Funkcja do animowania właściwości
	local function animateProperty(instance, propertyTable, duration)
		if instance then
			local tweenInfo = TweenInfo.new(
				duration, -- Czas trwania animacji
				Enum.EasingStyle.Quad, -- Styl animacji
				Enum.EasingDirection.Out -- Kierunek animacji
			)
			local tween = TweenService:Create(instance, tweenInfo, propertyTable)
			tween:Play()
		end
	end
	
	local function SundialTotem()
		
	end
	
	-- Funkcja toggle
	local function toggle()
		isActive = not isActive -- Zmiana stanu
	
		if isActive then
			animateProperty(frame, {Position = UDim2.new(0.66, 0, -0.043, 0)}, 0.3)
			clickButton.Visible = true
			task.spawn(SundialTotem) -- Uruchomienie funkcji
		else
			animateProperty(frame, {Position = UDim2.new(1.056, 0, -0.043, 0)}, 0.3)
			clickButton.Visible = false
		end
	end
	
	-- Podpięcie funkcji toggle do kliknięcia przycisku
	button.MouseButton1Click:Connect(function()
		toggle()
	end)
	
	clickButton.MouseButton1Click:Connect(function()
		toggle()
	end)
	
end
coroutine.wrap(BYMQJ_fake_script)()
local function IJUPH_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	-- Importowanie usług
	local TweenService = game:GetService("TweenService")
	local Players = game:GetService("Players")
	local VirtualInputManager = game:GetService("VirtualInputManager")
	local GuiService = game:GetService("GuiService")
	local ReplicatedStorage = game:GetService("ReplicatedStorage")
	local player = Players.LocalPlayer
	
	-- Referencje
	local toggleButton = script.Parent -- TextButton
	local movingFrame = script.Parent.Parent:FindFirstChild("Frame") -- Frame, który zmienia pozycję
	local frameTransparency = script.Parent.Parent.Parent:FindFirstChild("Frame") -- Ramka tła
	local dropdownSelected = script.Parent.Parent.Parent.Parent.Dropdown.Frame.TextLabel
	
	-- Zmienna przechowująca stan toggle
	local isActive = false
	
	-- Funkcja do animowania właściwości
	local function animateProperty(instance, propertyTable, duration)
		if instance then
			local tweenInfo = TweenInfo.new(duration, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
			local tween = TweenService:Create(instance, tweenInfo, propertyTable)
			tween:Play()
		end
	end
	
	-----------------------------------
	-- FUNKCJE AUTO -------------------
	-----------------------------------
	
	-- Funkcja WalkOnWater (zmienia CanCollide na wodzie)
	local function WalkOnWater()
		local fishingZone = workspace:FindFirstChild("zones") and workspace.zones:FindFirstChild("fishing")
		if fishingZone then
			for _, water in ipairs(fishingZone:GetChildren()) do
				if water:IsA("BasePart") then
					water.CanCollide = isActive -- Włącza/wyłącza kolizję w zależności od isActive
				end
			end
		end
	end
	
	-- AutoCatch: Obsługuje łapanie
	local function AutoCatch()
		-- Czekamy, aż pojawi się obiekt "reel" w PlayerGui
		while not (player:FindFirstChild("PlayerGui") and player.PlayerGui:FindFirstChild("reel")) do
			task.wait(0.5)
		end
	
		local reel = player.PlayerGui.reel
		if reel then
			while isActive do
				local args = { [1] = 100, [2] = true }
				ReplicatedStorage:WaitForChild("events"):WaitForChild("reelfinished"):FireServer(unpack(args))
				task.wait(0.3)
			end
		end
	end
	
	-- Funkcja do automatycznego wciskania klawiszy
	local function AutoShake()
		while isActive do
			local safeZone = player:FindFirstChild("PlayerGui")
				and player.PlayerGui:FindFirstChild("shakeui")
				and player.PlayerGui.shakeui:FindFirstChild("safezone")
	
			if safeZone and safeZone:FindFirstChild("button") then
				local button = safeZone.button
	
				-- Ustawienie focusa na przycisku
				GuiService.SelectedObject = button -- Ustawienie nawigacji na przycisk
				button.Selectable = true -- Umożliwienie wyboru przycisku
				task.wait(0.1) -- Krótki czas na ustawienie focusa
	
				-- Klikanie przycisku co 0.1 sekundy
				VirtualInputManager:SendKeyEvent(true, Enum.KeyCode.Return, false, game)
				VirtualInputManager:SendKeyEvent(false, Enum.KeyCode.Return, false, game)
				task.wait(0.1)
			else
				task.wait(0.1) -- Czekaj, jeśli przycisk nie istnieje
			end
		end
	end
	
	local function AutoCast()
		while isActive do  -- Pętla, która sprawdza stan co 0.1 sekundy
			local hud = player:FindFirstChild("PlayerGui") and player.PlayerGui:FindFirstChild("hud")
			if hud and hud.safezone and hud.safezone.backpack and hud.safezone.backpack.hotbar then
				local hotbarSlot = hud.safezone.backpack.hotbar["1"]
				if hotbarSlot then
					local itemNameObject = hotbarSlot:FindFirstChild("Tool")
					local rodName = itemNameObject and tostring(itemNameObject.Value)
					if rodName then
						-- Sprawdzamy, czy gracz ma wędkę w plecaku
						local rodInBackpack = player.Backpack:FindFirstChild(rodName)
	
						-- Jeśli wędka jest w plecaku, wyposażamy ją
						if rodInBackpack then
							VirtualInputManager:SendKeyEvent(true, Enum.KeyCode.One, false, game)
							task.wait(0.2)
							VirtualInputManager:SendKeyEvent(false, Enum.KeyCode.One, false, game)
							task.wait(0.2) -- Czekamy chwilę, aby uniknąć błędów
						end
	
						-- Sprawdzamy, czy wędka jest już w ręce
						local rod = player.Character:FindFirstChild(rodName)
						if rod and rod:FindFirstChild("events") and rod.events:FindFirstChild("cast") then
							-- Jeśli rod.events.cast istnieje, wywołujemy RemoteEvent
							local args = {
								[1] = 100, -- Rzucanie wędką
								[2] = 1    -- Opcjonalny parametr
							}
							rod.events.cast:FireServer(unpack(args))
							task.wait(0.2) -- Czekamy chwilę przed następnym rzutem
						end
					end
				end
			end
			task.wait(0.2)  -- Oczekiwanie 0.1 sekundy przed kolejną iteracją
		end
	end
	
	-- Funkcja teleportująca gracza na 5 studs wyżej niż "megDefault"
	local function playertp(target)
		local character = player.Character
		local humanoidRootPart = character and character:FindFirstChild("HumanoidRootPart")
	
		if humanoidRootPart and target then
			-- Sprawdź, czy gracz jest już wystarczająco daleko od target
			local distance = (humanoidRootPart.Position - target.Position).Magnitude
			if distance > 30 then
				-- Telegrafuj gracza 5 studs wyżej od target
				local newPosition = target.Position + Vector3.new(0, 5, 0)
				humanoidRootPart.CFrame = CFrame.new(newPosition)
	
				-- Telegrafuj gracza ponownie w to samo miejsce, aby upewnić się, że trafił dokładnie
				task.wait(0.1)
				humanoidRootPart.CFrame = CFrame.new(target.Position)
			end
		end
	end
	
	
	local configPath = "BolboHubConfig.txt"
	
	local function saveConfig(settings)
		local data = ""
		for key, value in pairs(settings) do
			data = data .. key .. "=" .. tostring(value) .. "\n"
		end
		writefile(configPath, data)
	end
	
	local function loadConfig()
		local settings = {}
	
		if isfile(configPath) then
			local config = readfile(configPath)
			for line in config:gmatch("[^\r\n]+") do
				local k, v = line:match("([^=]+)=(.+)")
				if k and v then
					if v == "true" then
						settings[k] = true
					elseif v == "false" then
						settings[k] = false
					else
						settings[k] = tonumber(v) or v -- Zamienia liczby na numeryczne wartości
					end
				end
			end
		end
	
		return settings
	end
	
	local function updateConfig(key, newValue)
		local settings = loadConfig()
		settings[key] = newValue
		saveConfig(settings)
	end
	
	
	local config = loadConfig()
	isActive = config["AutoFishSeaEvent"] or false -- Domyślnie false, jeśli brak wartości
	
	if isActive then
		-- Aktualizacja UI
		animateProperty(frameTransparency, {BackgroundTransparency = 0}, 0.3)
		animateProperty(movingFrame, {Position = UDim2.new(0.525, 0, 0, 0)}, 0.3)
		wait(1)
	
		-- Uruchomienie odpowiednich funkcji, jeśli było aktywne
		if dropdownSelected.Text == "Megalodon Default" then
			spawn(function()
				while isActive do
					local megDefault = workspace.zones.fishing["Megalodon Default"]
					if megDefault then
						task.spawn(function() playertp(megDefault) end)
						task.spawn(AutoCast)
						task.spawn(AutoShake)
						task.spawn(AutoCatch)
					else
						break
					end
					task.wait(2)
				end
			end)
		end
	
		WalkOnWater()
	end
	
	
	local function toggle()
		isActive = not isActive
		updateConfig("AutoFishSeaEvent", isActive)
	
		-- Aktualizacja UI
		if isActive then
			animateProperty(frameTransparency, {BackgroundTransparency = 0}, 0.3)
			animateProperty(movingFrame, {Position = UDim2.new(0.525, 0, 0, 0)}, 0.3)
	
			if dropdownSelected.Text == "Megalodon Default" then
				spawn(function()
					while isActive do
						local megDefault = workspace.zones.fishing["Megalodon Default"]
						if megDefault then
							task.spawn(function()
								playertp(megDefault)
							end)
							task.spawn(AutoCast)
							task.spawn(AutoShake)
							task.spawn(AutoCatch)
						else
							break
						end
						task.wait(2)
					end
				end)
			end
		else
			animateProperty(frameTransparency, {BackgroundTransparency = 1}, 0.3)
			animateProperty(movingFrame, {Position = UDim2.new(0, 0, 0, 0)}, 0.3)
		end
	
		WalkOnWater()
	end
	
	
	-- Podpięcie funkcji toggle do kliknięcia przycisku
	toggleButton.MouseButton1Click:Connect(toggle)
	
end
coroutine.wrap(IJUPH_fake_script)()
local function MYXXYZO_fake_script() -- TextButton_2.LocalScript 
	local script = Instance.new('LocalScript', TextButton_2)

	-- Importowanie usług
	local TweenService = game:GetService("TweenService")
	local Players = game:GetService("Players")
	local VirtualInputManager = game:GetService("VirtualInputManager")
	local GuiService = game:GetService("GuiService")
	
	-- Referencje
	local toggleButton = script.Parent -- TextButton
	local movingFrame = script.Parent.Parent:FindFirstChild("Frame") -- Frame, który zmienia pozycję
	local frameTransparency = script.Parent.Parent.Parent:FindFirstChild("Frame") -- Poprawne odniesienie do ramki tła
	local player = Players.LocalPlayer
	local dropdownSelected = script.Parent.Parent.Parent.Parent.Dropdown.Frame.TextLabel
	
	-- Ścieżka do pliku konfiguracji
	local configPath = "BolboHubConfig.txt"
	
	-- Zmienna przechowująca stan toggle
	local isActive = false
	local placeId = 16732694052
	
	-- Funkcja do animowania właściwości
	local function animateProperty(instance, propertyTable, duration)
		if instance then
			local tweenInfo = TweenInfo.new(duration, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
			local tween = TweenService:Create(instance, tweenInfo, propertyTable)
			tween:Play()
		end
	end
	
	-- Funkcja AutoHop
	local function AutoHop()
		while isActive do
			wait(7)
			local servers = game:GetService("HttpService"):JSONDecode(game:HttpGet("https://games.roblox.com/v1/games/" .. placeId .. "/servers/Public?sortOrder=Asc&limit=100"))
			for _, server in pairs(servers.data) do
				if server.id ~= game.JobId and server.playing < server.maxPlayers then
					game:GetService("TeleportService"):TeleportToPlaceInstance(placeId, server.id)
					return
				end
			end
		end
	end
	
	-- Funkcja odczytu pliku i zwracania wartości
	local function getConfigValue(key)
		if not isfile(configPath) then return nil end
		local config = readfile(configPath)
		for line in config:gmatch("[^\r\n]+") do
			local k, v = line:match("^(.-)=(.-)$")
			if k and v and k == key then
				return v:gsub("^%s*(.-)%s*$", "%1") -- Usuwa spacje na początku i końcu
			end
		end
		return nil
	end
	
	-- Funkcja zapisu wartości do pliku
	local function setConfigValue(key, value)
		local configData = {}
		if isfile(configPath) then
			for line in readfile(configPath):gmatch("[^\r\n]+") do
				local k, v = line:match("^(.-)=(.-)$")
				if k and v and k ~= key then
					table.insert(configData, k .. "=" .. v)
				end
			end
		end
		table.insert(configData, key .. "=" .. value)
		writefile(configPath, table.concat(configData, "\n"))
	end
	
	-- Funkcja wczytania konfiguracji
	local function loadConfig()
		local savedState = getConfigValue("AutoHopToggle")
		if savedState then
			isActive = savedState == "true"
	
			-- Jeśli było aktywne przed restartem, to uruchamiamy od razu
			if isActive then
				animateProperty(frameTransparency, {BackgroundTransparency = 0}, 0.3)
				animateProperty(movingFrame, {Position = UDim2.new(0.525, 0, 0, 0)}, 0.3)
				wait(7)
				if dropdownSelected.Text == "Megalodon Default" then
					spawn(function()
						while isActive do
							local megDefault = workspace.zones.fishing:FindFirstChild("Megalodon Default")
							if not megDefault then
								AutoHop()
							end
							wait(2)
						end
					end)
				end
			end
		end
	end
	
	-- Funkcja toggle
	local function toggle()
		isActive = not isActive -- Zmiana stanu
		setConfigValue("AutoHopToggle", tostring(isActive)) -- Zapisujemy zmianę stanu
	
		if isActive then
			animateProperty(frameTransparency, {BackgroundTransparency = 0}, 0.3)
			animateProperty(movingFrame, {Position = UDim2.new(0.525, 0, 0, 0)}, 0.3)
	
			if dropdownSelected.Text == "Megalodon Default" then
				spawn(function()
					while isActive do
						local megDefault = workspace.zones.fishing:FindFirstChild("Megalodon Default")
						if not megDefault then
							AutoHop()
						end
						wait(2)
					end
				end)
			end
		else
			animateProperty(frameTransparency, {BackgroundTransparency = 1}, 0.3)
			animateProperty(movingFrame, {Position = UDim2.new(0, 0, 0, 0)}, 0.3)
		end
	end
	
	-- Wczytanie konfiguracji przy starcie
	loadConfig()
	
	-- Podpięcie funkcji toggle do kliknięcia przycisku
	toggleButton.MouseButton1Click:Connect(toggle)
	
end
coroutine.wrap(MYXXYZO_fake_script)()
local function JRFJGHN_fake_script() -- TextButton_3.LocalScript 
	local script = Instance.new('LocalScript', TextButton_3)

	-- Importowanie TweenService
	local TweenService = game:GetService("TweenService")
	
	local categoryButtons = script.Parent.Parent -- Rodzic dla wszystkich przycisków kategorii
	local selectedLabel = categoryButtons.Parent.Dropdown.Frame.TextLabel -- text ktory sie zmienia jesli klikne cos z tabeli
	local buttonLabel = script.Parent.ButtonLabel
	
	-- Ścieżka do pliku konfiguracji
	local configPath = "BolboHubConfig.txt"
	
	-- Funkcja zapisu konfiguracji
	local function saveConfig(selectedOption)
		local configData = readfile(configPath) or "" -- Pobiera istniejące dane, jeśli plik istnieje
		configData = configData:gsub("SeaEventDropdown=.*", "SeaEventDropdown=" .. selectedOption) -- Zamienia tylko tę linijkę
		writefile(configPath, configData)
	end
	
	-- Funkcja odczytu konfiguracji
	local function loadConfig()
		if isfile(configPath) then
			local config = readfile(configPath)
			local savedOption = config:match("SeaEventDropdown=([^\n\r]+)") -- Pobiera tylko jedną linię
	
			if savedOption then
				selectedLabel.Text = savedOption
				-- Zmienia wygląd przycisku, który odpowiada zapisanej opcji
				for _, button in pairs(categoryButtons:GetChildren()) do
					if button:IsA("TextButton") and button.ButtonLabel.Text == savedOption then
						animateTransparency(button, 0.6, 0.3)
					else
						animateTransparency(button, 1, 0.3)
					end
				end
			end
		else
			writefile(configPath, "SeaEventDropdown=None")
		end
	end
	
	-- Funkcja do animowania przezroczystości
	local function animateTransparency(guiObject, targetTransparency, duration)
		local tweenInfo = TweenInfo.new(duration, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
		local tween = TweenService:Create(guiObject, tweenInfo, {BackgroundTransparency = targetTransparency})
		tween:Play()
	end
	
	-- Funkcja zmiany kategorii
	local function changeCategory()
		local newOption = buttonLabel.Text
		saveConfig(newOption) -- Zapisz wybraną opcję
	
		-- Aktualizuje UI, podświetlając wybraną opcję
		for _, button in pairs(categoryButtons:GetChildren()) do
			if button:IsA("TextButton") then
				if button == script.Parent then
					selectedLabel.Text = newOption
					animateTransparency(button, 0.6, 0.3) -- Podświetl kliknięty przycisk
				else
					animateTransparency(button, 1, 0.3) -- Przywróć resztę do domyślnego wyglądu
				end
			end
		end
	end
	
	-- Wczytaj zapisane ustawienia po starcie
	loadConfig()
	
	-- Podpięcie funkcji do kliknięcia
	script.Parent.MouseButton1Click:Connect(changeCategory)
	
end
coroutine.wrap(JRFJGHN_fake_script)()
local function BHRAK_fake_script() -- TextButton_4.LocalScript 
	local script = Instance.new('LocalScript', TextButton_4)

	-- Importowanie usług
	local TweenService = game:GetService("TweenService")
	local Players = game:GetService("Players")
	local VirtualInputManager = game:GetService("VirtualInputManager")
	local GuiService = game:GetService("GuiService")
	
	-- Zmienna ścieżki do pliku konfiguracji
	local configPath = "BolboHubConfig.txt"
	
	-- Referencje do UI
	local toggleButton = script.Parent -- TextButton
	local movingFrame = script.Parent.Parent:FindFirstChild("Frame") -- Frame, który zmienia pozycję
	local frameTransparency = script.Parent.Parent.Parent:FindFirstChild("Frame") -- Poprawne odniesienie do ramki tła
	local player = Players.LocalPlayer
	
	-- Zmienna przechowująca stan toggle
	local isActive = false
	
	-- Funkcja do animowania właściwości
	local function animateProperty(instance, propertyTable, duration)
		if instance then
			local tweenInfo = TweenInfo.new(duration, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
			local tween = TweenService:Create(instance, tweenInfo, propertyTable)
			tween:Play()
		end
	end
	
	-- Funkcja do automatycznego wciskania klawiszy
	local function AutoShake()
		while isActive do
			local safeZone = player:FindFirstChild("PlayerGui")
				and player.PlayerGui:FindFirstChild("shakeui")
				and player.PlayerGui.shakeui:FindFirstChild("safezone")
	
			if safeZone and safeZone:FindFirstChild("button") then
				local button = safeZone.button
				GuiService.SelectedObject = button -- Ustawienie nawigacji na przycisk
				button.Selectable = true -- Umożliwienie wyboru przycisku
				task.wait(0.05) -- Krótki czas na ustawienie focusa
				VirtualInputManager:SendKeyEvent(true, Enum.KeyCode.Return, false, game)
				VirtualInputManager:SendKeyEvent(false, Enum.KeyCode.Return, false, game)
				task.wait(0.05)
			else
				task.wait(0.05) -- Czekaj, jeśli przycisk nie istnieje
			end
		end
	end
	
	
	-- Funkcja toggle
	local function toggle()
		isActive = not isActive -- Zmiana stanu
	
		-- Animacja UI
		if isActive then
			animateProperty(frameTransparency, {BackgroundTransparency = 0}, 0.3)
			animateProperty(movingFrame, {Position = UDim2.new(0.525, 0, 0, 0)}, 0.3)
			task.spawn(AutoShake)
		else
			animateProperty(frameTransparency, {BackgroundTransparency = 1}, 0.3)
			animateProperty(movingFrame, {Position = UDim2.new(0, 0, 0, 0)}, 0.3)
		end
	end
	
	-- Podpięcie funkcji toggle do kliknięcia przycisku
	toggleButton.MouseButton1Click:Connect(toggle)
	
end
coroutine.wrap(BHRAK_fake_script)()
local function DLPAFGY_fake_script() -- TextButton_5.LocalScript 
	local script = Instance.new('LocalScript', TextButton_5)

	-- Importowanie usług
	local TweenService = game:GetService("TweenService")
	local Players = game:GetService("Players")
	local VirtualInputManager = game:GetService("VirtualInputManager")
	local GuiService = game:GetService("GuiService")
	
	-- Referencje
	local toggleButton = script.Parent -- TextButton
	local movingFrame = script.Parent.Parent:FindFirstChild("Frame") -- Frame, który zmienia pozycję
	local frameTransparency = script.Parent.Parent.Parent:FindFirstChild("Frame") -- Poprawne odniesienie do ramki tła
	local player = Players.LocalPlayer
	
	-- Zmienna przechowująca stan toggle
	local isActive = false
	
	-- Funkcja do animowania właściwości
	local function animateProperty(instance, propertyTable, duration)
		if instance then
			local tweenInfo = TweenInfo.new(
				duration, -- Czas trwania animacji
				Enum.EasingStyle.Quad, -- Styl animacji
				Enum.EasingDirection.Out -- Kierunek animacji
			)
			local tween = TweenService:Create(instance, tweenInfo, propertyTable)
			tween:Play()
		end
	end
	
	local function AutoCatch()
		-- Sprawdzamy, czy obiekt "reel" istnieje, jeśli nie, czekamy na niego
		while not player:FindFirstChild("PlayerGui") or not player.PlayerGui:FindFirstChild("reel") do
			task.wait(0.5)  -- Czekamy 0.5 sekundy, zanim spróbujemy ponownie
		end
	
		-- Gdy "reel" istnieje, wykonujemy dalszą logikę
		local reel = player.PlayerGui.reel
		if reel then
			-- Rozpoczęcie głównej logiki, kiedy reel jest już dostępny
			while isActive do
				local args = {
					[1] = 100,
					[2] = true
				}
	
				-- Wywołanie eventu
				game:GetService("ReplicatedStorage"):WaitForChild("events"):WaitForChild("reelfinished"):FireServer(unpack(args))
				task.wait(0.3)  -- Czekamy 0.3 sekundy przed kolejnym wykonaniem
			end
		end
	end
	
	-- Funkcja toggle
	local function toggle()
		isActive = not isActive -- Zmiana stanu
	
		if isActive then
			animateProperty(frameTransparency, {BackgroundTransparency = 0}, 0.3)
			animateProperty(movingFrame, {Position = UDim2.new(0.525, 0, 0, 0)}, 0.3) -- Przesunięcie
			task.spawn(AutoCatch) -- Uruchomienie funkcji AutoShake
		else
			animateProperty(frameTransparency, {BackgroundTransparency = 1}, 0.3)
			animateProperty(movingFrame, {Position = UDim2.new(0, 0, 0, 0)}, 0.3) -- Przesunięcie
		end
	end
	
	-- Podpięcie funkcji toggle do kliknięcia przycisku
	toggleButton.MouseButton1Click:Connect(toggle)
	
end
coroutine.wrap(DLPAFGY_fake_script)()
local function JLMOKRZ_fake_script() -- TextButton_6.LocalScript 
	local script = Instance.new('LocalScript', TextButton_6)

	-- Importowanie usług
	local TweenService = game:GetService("TweenService")
	local Players = game:GetService("Players")
	local VirtualInputManager = game:GetService("VirtualInputManager")
	local GuiService = game:GetService("GuiService")
	
	-- Referencje
	local toggleButton = script.Parent -- TextButton
	local movingFrame = script.Parent.Parent:FindFirstChild("Frame") -- Frame, który zmienia pozycję
	local frameTransparency = script.Parent.Parent.Parent:FindFirstChild("Frame") -- Poprawne odniesienie do ramki tła
	local player = Players.LocalPlayer
	
	-- Zmienna przechowująca stan toggle
	local isActive = false
	
	-- Funkcja do animowania właściwości
	local function animateProperty(instance, propertyTable, duration)
		if instance then
			local tweenInfo = TweenInfo.new(
				duration, -- Czas trwania animacji
				Enum.EasingStyle.Quad, -- Styl animacji
				Enum.EasingDirection.Out -- Kierunek animacji
			)
			local tween = TweenService:Create(instance, tweenInfo, propertyTable)
			tween:Play()
		end
	end
	
	local function AutoCast()
		while isActive do  -- Pętla, która sprawdza stan co 0.1 sekundy
			local hud = player:FindFirstChild("PlayerGui") and player.PlayerGui:FindFirstChild("hud")
			if hud and hud.safezone and hud.safezone.backpack and hud.safezone.backpack.hotbar then
				local hotbarSlot = hud.safezone.backpack.hotbar["1"]
				if hotbarSlot then
					local itemNameObject = hotbarSlot:FindFirstChild("Tool")
					local rodName = itemNameObject and tostring(itemNameObject.Value)
					if rodName then
						-- Sprawdzamy, czy gracz ma wędkę w plecaku
						local rodInBackpack = player.Backpack:FindFirstChild(rodName)
	
						-- Jeśli wędka jest w plecaku, wyposażamy ją
						if rodInBackpack then
							VirtualInputManager:SendKeyEvent(true, Enum.KeyCode.One, false, game)
							task.wait(0.2)
							VirtualInputManager:SendKeyEvent(false, Enum.KeyCode.One, false, game)
							task.wait(0.2) -- Czekamy chwilę, aby uniknąć błędów
						end
	
						-- Sprawdzamy, czy wędka jest już w ręce
						local rod = player.Character:FindFirstChild(rodName)
						if rod and rod:FindFirstChild("events") and rod.events:FindFirstChild("cast") then
							-- Jeśli rod.events.cast istnieje, wywołujemy RemoteEvent
							local args = {
								[1] = 100, -- Rzucanie wędką
								[2] = 1    -- Opcjonalny parametr
							}
							rod.events.cast:FireServer(unpack(args))
							task.wait(0.2) -- Czekamy chwilę przed następnym rzutem
						end
					end
				end
			end
			task.wait(0.2)  -- Oczekiwanie 0.1 sekundy przed kolejną iteracją
		end
	end
	
	
	
	-- Funkcja toggle
	local function toggle()
		isActive = not isActive -- Zmiana stanu
	
		if isActive then
			animateProperty(frameTransparency, {BackgroundTransparency = 0}, 0.3)
			animateProperty(movingFrame, {Position = UDim2.new(0.525, 0, 0, 0)}, 0.3) -- Przesunięcie
			task.spawn(AutoCast)
		else
			animateProperty(frameTransparency, {BackgroundTransparency = 1}, 0.3)
			animateProperty(movingFrame, {Position = UDim2.new(0, 0, 0, 0)}, 0.3) -- Przesunięcie
		end
	end
	
	-- Podpięcie funkcji toggle do kliknięcia przycisku
	toggleButton.MouseButton1Click:Connect(toggle)
	
end
coroutine.wrap(JLMOKRZ_fake_script)()
local function PZWAO_fake_script() -- TextButton_7.LocalScript 
	local script = Instance.new('LocalScript', TextButton_7)

	-- Importowanie usług
	local TweenService = game:GetService("TweenService")
	local Players = game:GetService("Players")
	
	-- Referencje
	local toggleButton = script.Parent -- TextButton
	local movingFrame = script.Parent.Parent:FindFirstChild("Frame") -- Frame, który zmienia pozycję
	local frameTransparency = script.Parent.Parent.Parent:FindFirstChild("Frame") -- Poprawne odniesienie do ramki tła
	local player = Players.LocalPlayer
	
	-- Zmienna przechowująca stan toggle
	local isActive = false
	
	-- Funkcja do animowania właściwości
	local function animateProperty(instance, propertyTable, duration)
		if instance then
			local tweenInfo = TweenInfo.new(duration, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
			local tween = TweenService:Create(instance, tweenInfo, propertyTable)
			tween:Play()
		end
	end
	
	-- Funkcja WalkOnWater (zmienia CanCollide na wodzie)
	local function WalkOnWater()
		local fishingZone = workspace:FindFirstChild("zones") and workspace.zones:FindFirstChild("fishing")
		if fishingZone then
			for _, water in ipairs(fishingZone:GetChildren()) do
				if water:IsA("BasePart") then
					water.CanCollide = isActive -- Włącza/wyłącza kolizję w zależności od isActive
				end
			end
		end
	end
	
	-- Funkcja toggle
	local function toggle()
		isActive = not isActive -- Zmiana stanu
	
		if isActive then
			animateProperty(frameTransparency, {BackgroundTransparency = 0}, 0.3)
			animateProperty(movingFrame, {Position = UDim2.new(0.525, 0, 0, 0)}, 0.3)
		else
			animateProperty(frameTransparency, {BackgroundTransparency = 1}, 0.3)
			animateProperty(movingFrame, {Position = UDim2.new(0, 0, 0, 0)}, 0.3)
		end
	
		WalkOnWater() -- Zawsze aktualizuje stan wodnych partów po zmianie isActive
	end
	
	-- Podpięcie funkcji toggle do kliknięcia przycisku
	toggleButton.MouseButton1Click:Connect(toggle)
	
end
coroutine.wrap(PZWAO_fake_script)()
local function DAEQR_fake_script() -- BolboHubImage.LocalScript 
	local script = Instance.new('LocalScript', BolboHubImage)

	local UserInputService = game:GetService("UserInputService")
	local imageButton = script.Parent
	
	local dragging = false
	local dragStart
	local startPos
	
	local function update(input)
		local delta = input.Position - dragStart
		imageButton.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
	end
	
	imageButton.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			dragging = true
			dragStart = input.Position
			startPos = imageButton.Position
	
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	
	UserInputService.InputChanged:Connect(function(input)
		if dragging and input.UserInputType == Enum.UserInputType.MouseMovement then
			update(input)
		end
	end)
	
	function showGui()
		local player = game.Players.LocalPlayer
		if player then
			local gui = script.Parent.Parent:FindFirstChild("Window")
			if gui then
				gui.Visible = true
			end
		end
	end
	
	function HideBolboImage()
		local player = game.Players.LocalPlayer
		if player then
			local image = script.Parent
			if image then
				image.Visible = false
			end
		end
	end
	
	imageButton.MouseButton1Click:Connect(HideBolboImage)
	imageButton.MouseButton1Click:Connect(showGui)
end
coroutine.wrap(DAEQR_fake_script)()

local copyButton = CoreGui.BolboHub.Window.Categories.Frame1.GetKey
local verifyButton = CoreGui.BolboHub.Window.Categories.Frame1.SubmitKey
local keyBox = CoreGui.BolboHub.Window.Categories.Frame1.TextBox

copyButton.MouseButton1Click:Connect(function()
	copyLink();
end)

verifyButton.MouseButton1Click:Connect(function()
	local key = keyBox.Text;
	local success = verifyKey(key);

	if success then
		print("key is valid.");
	else
		print("key is invalid.");
	end
end)

local flag = getFlag("example_flag");
if flag ~= nil then
	print("flag value: " .. flag);
else
	print("failed to get flag.");
end
